{
    "name": "aquagas-flow-api",
    "version": "0.0.0",
    "scripts": {
        "build": "npx ts-node build.ts",
        "lint": "npx eslint src/",
        "lint:fix": "npx eslint src/ --fix",
        "lint:tests": "npx eslint spec/",
        "lint:tests:fix": "npx eslint spec/ --fix",
        "start": "node -r module-alias/register ./dist/server.js --env=production",
        "dev": "nodemon",
        "test": "nodemon --config ./spec/nodemon.json",
        "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts, html",
        "ignore": [
            "src/public"
        ],
        "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src/server.ts"
    },
    "_moduleAliases": {
        "@src": "dist"
    },
    "engines": {
        "node": ">=8.10.0"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^7.1.0",
        "hpp": "^0.2.3",
        "inserturlparams": "^2.0.4",
        "jet-logger": "^1.3.1",
        "jet-paths": "^1.0.9",
        "jet-validator": "^1.1.1",
        "jsonfile": "^6.1.0",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "sharp": "^0.33.5",
        "ts-command-line-args": "^2.5.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.9.1",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/find": "^0.2.4",
        "@types/fs-extra": "^11.0.4",
        "@types/hpp": "^0.2.6",
        "@types/jasmine": "^5.1.4",
        "@types/jsonfile": "^6.1.4",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.5.0",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.3.0",
        "@typescript-eslint/parser": "^8.3.0",
        "eslint": "^9.9.1",
        "eslint-plugin-node": "^11.1.0",
        "find": "^0.3.0",
        "fs-extra": "^11.2.0",
        "jasmine": "^5.2.0",
        "nodemon": "^3.1.4",
        "supertest": "^7.0.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.5.4"
    }
}
